    @model Metasoft.Models.ClienteContaPrViewModel
    @{ViewBag.Title = "Nota Fiscal a Receber";}

    @using (Html.BeginForm(Html.BeginForm(null, null, FormMethod.Post, new { id = "NotaFiscalAReceber" })))
    {
        <style>
            .title1{
                    font-size:20px;
                    text-align:center;
                    font-weight:bold;
            }
            .title2 {
                    font-size:14px;
                    text-align:center;
             }
            .blab{border:solid 0.3px black; border-collapse:collapse;}
            .brdt{border-top:solid 0.3px black}
            .brdb{border-bottom:solid 0.3px black}
            .brdr{border-right:solid 0.3px black}
            .brdl{border-left:solid 0.3px black}
            .h90{height:90px;}
            .H35{height:45px;}
            .padl10{padding-left:10px;}
            .padl05{padding-left:5px;}
            .fwb{font-weight:bold;}
            .tac {text-align:center;}
            .form-control-nf {
                font-size: 12px;
                color: #061568;
                width: 97%;
                border-radius: 4px;
                background-color: #8dc7f4;
                height: 27px;
            }
         
        </style>

        <div class="panel panel-default mgauto dspnone" style="margin-top:50px;width:100%;">
            <br />
            <br />
            <div class="panel-heading">
                <div class="row">
                    <div id="pagetitle" class="col-md-3">
                        Emissão de Nota Fiscal
                    </div>
                    <div class="col-md-6 noprint">


                    </div>
                    <div class="col-md-3 noprint">
                        <a id="printicon" class="topicons dspnone noprint" onclick="Imprime();" title="Imprimir"><i class="fa fa-print"></i></a>
                    </div>
                </div>
            </div>
            <br />
            <div class="container mgauto" style="background-color:#45a3e4;width:97%;">
                <!-- header linha 1 -->
                @Html.HiddenFor(model => model.cliforid)
                @Html.HiddenFor(model => model.contaprid)
                @Html.HiddenFor(model => model.vencimento)
                @Html.HiddenFor(model => model.noordem)
                <!-------------------------------------------------------------
                    HEADER LOGO, ENDEREÇO
                  ------------------------------------------------------------>
                <div id="nfheaderl1" class="row brdb brdl brdr">
                    <div class="col-md-3 brdt brdr h90">
                        logo
                    </div>
                    <!-- pega do Web config  ? -->
                    <div class="col-md-9 brdt h90">
                        <div class="title1">DNK INFOTELECOM PROGRAMAÇÃO E ANALISE EM INFORMATICA LTDA</div>
                        <div class="title2">ENDEREÇO: AV.  ENGENHEIRO LUIZ CARLOS BERRINI, 1748, CONJ. 401, 402, 403, 404, CIDADE MONÇÕES</div>
                        <div>
                            <div class="row">
                                <div class="col-md-3 title2">CEP: 04602-000</div>
                                <div class="col-md-6"></div>
                                <div class="col-md-3 title2">SÃO PAULO - SP</div>
                            </div>
                        </div>
                    </div>

                </div>
                <!-------------------------------------------------------------
                   INCRICAO MUNICIPAL CNPJ NOTA DE LOCACAO DE B ENS MOVEIS
                 ------------------------------------------------------------>
                <div id="nfheaderl2" class="row brdb brdl brdr">
                    <div class="col-md-3 brdr">
                        <div class="row">
                            <div class="col-md-12">
                                <span style="font-size:12px;">INSC. MUNICIPAL:</span>&nbsp;
                                <span style="font-size:12px;">@Html.DisplayFor(model => model.ie, new { @class = "form-control form-control-nf" })</span>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <span style="font-size:12px;">CNPJ:</span>&nbsp;
                                <span style="font-size:12px;">@Html.DisplayFor(model => model.cnpj, new { @class = "form-control form-control-nf" })</span>
                            </div>

                        </div>
                    </div>
                    <div class="col-md-9">
                        <div class="row brdb">
                            <div class="col-md-12">
                                <div class="mgauto" style="width:35%;">
                                    NOTA DE LOCAÇÃO DE BENS MÓVEIS
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="mgauto" style="width:35%;">
                                    1º VIA CLIENTE
                                </div>
                            </div>
                            <div class="col-md-6">
                                <span style="font-size:12px;">Nota Fiscal nº</span>&nbsp;
                                <span style="font-size:12px;">@Html.DisplayFor(model => model.nfid, new { @class = "form-control form-control-nf" })</span>
                            </div>
                        </div>
                    </div>
                </div>

                <!-------------------------------------------------------------
                  EMISSAO , Nº ORDEM, LOCACAO DE BENS MOVEIS VENCIMENTO
                ------------------------------------------------------------>
                <div id="nfheaderl3" class="row brdb brdl brdr">
                    <div class="col-md-3 brdr">
                        <div class="row">
                            <div class="col-md-12">
                                <span style="font-size:14px;">Emissão:</span>&nbsp;
                                <span style="font-size:14px;">@Html.DisplayFor(model => model.emissao, new { @class = "form-control form-control-nf" })</span>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <span style="font-size:14px;">Nº Ordem:</span>&nbsp;
                                <span style="font-size:14px;">@Html.DisplayFor(model => model.noordem, new { @class = "form-control form-control-nf" })</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-9">
                        <div class="row">
                            <div class="col-md-12 brdb">
                                <div class="mgauto" style="width:59%;">
                                    NATUREZA DA OPERAÇÃO:&nbsp;LOCAÇÃO DE BENS MÓVEIS
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="mgauto" style="width:25%;">
                                    <span style="font-size:14px;">Vencimento:</span>&nbsp;
                                    <span style="font-size:14px;">@Html.DisplayFor(model => model.vencimento, new { @class = "form-control form-control-nf" })</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-------------------------------------------------------------
                      DESTINATARIO
                   ------------------------------------------------------------>
                <div id="nfheaderl4" class="row brdb brdl brdr">
                    <div class="col-md-12">
                        <span style="font-size:14px;">Destinatário:</span>&nbsp;
                        <span style="font-size:14px;">@Html.DisplayFor(model => model.clientenome, new { @class = "form-control form-control-nf" })</span>
                
                    </div>
                </div>
                <!-------------------------------------------------------------
                     ENDERECO , BAIRRO
                  ------------------------------------------------------------>
                <div id="nfheaderl5" class="row brdb brdl brdr">
                    <div class="col-md-7">
                        <span style="font-size:14px;">Endereço:</span>&nbsp;
                        <span style="font-size:14px;">@Html.DisplayFor(model => model.endereco, new { @class = "form-control form-control-nf" })</span>
                    </div>
                    <div class="col-md-5">
                        <span style="font-size:14px;">Bairro:</span>&nbsp;
                        <span style="font-size:14px;">@Html.DisplayFor(model => model.bairro, new { @class = "form-control form-control-nf" })</span>
                    </div>
 
                </div>
                <div id="nfheaderl5" class="row brdb brdl brdr">
                    <div class="col-md-6">
                        <span style="font-size:14px;">Município:</span>&nbsp;
                        <span style="font-size:14px;">@Html.DisplayFor(model => model.cidade, new { @class = "form-control form-control-nf" })</span>
                    </div>
                    <div class="col-md-3">
                        <span style="font-size:14px;">Estado:</span>&nbsp;
                        <span style="font-size:14px;">@Html.DisplayFor(model => model.estado, new { @class = "form-control form-control-nf" })</span>
                    </div>
                    <div class="col-md-3">
                        <span style="font-size:14px;">CEP:</span>&nbsp;
                        <span style="font-size:14px;">@Html.DisplayFor(model => model.cep, new { @class = "form-control form-control-nf" })</span>
                    </div>
                    
                </div>
                <!-------------------------------------------------------------
                     ITENS DA NOTA FISCAL
                  ------------------------------------------------------------>
                <div id="nfheaderl5" class="row brdl brdr">
                    <div class="row  brdb ">
                        <div class="col-md-1">
                            <span style="font-size:14px;">Quantidade</span>
                        </div>
                        <div class="col-md-7">
                            <span style="font-size:14px;">Descrição</span>
                        </div>
                        <div class="col-md-2">
                            <span style="font-size:14px;">R$ Unitário</span>
                        </div>
                        <div class="col-md-2">
                            <span style="font-size:14px;">R$ Total</span>
                        </div>
                    </div>
                   @foreach (var item in Model.nfdetails)
                   {

                    string qtd = (item.quantidade == 0) ? "" : item.quantidade.ToString();
                    string unitario = (item.unitario == 0) ? "" : item.unitario.ToString();
                    string total = (item.total == 0) ? "" : item.total.ToString();
                    <div  class="row brdb">
                        <div class="col-md-1 brdr">
                            <span style="font-size:14px;">&nbsp;@qtd</span>
                        </div>
                        <div class="col-md-7 brdr">
                            <span style="font-size:14px;">&nbsp;@Html.DisplayFor(model => item.produtonome, new { @class = "form-control form-control-nf" })</span>
                        </div>
                        <div class="col-md-2 brdr">
                            <span style="font-size:14px;">&nbsp;@unitario</span>
                        </div>
                        <div class="col-md-2">
                            <span style="font-size:14px;">&nbsp;@total</span>
                        </div>
                    </div>
                   }
                </div>
            </div>
        </div>



    }

    @section Scripts {
        <script type="text/javascript">
            $(document).ready(function () {
                Animation();
          
                for (var i = 0; i <= 19; i++) {
                    $('#qtd' + i).mask("000", { reverse: true });
                    $("#emissao").mask("00/00/0000");
                    $('#unitario'+ i).mask("###0,00", { reverse: true });
           
                }
            });
   
      
        


        </script>
        @Scripts.Render("~/Scripts/bootstrap-datepicker.min.js")
        @Scripts.Render("~/Scripts/locales/bootstrap-datepicker.pt-BR.min.js")

    }


